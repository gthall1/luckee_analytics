<div class="col-md-12">
    <p class="text-center">
        <strong>Monthly Goals</strong>
    </p>
    <%= render partial: "/common/goal", :locals => {name: "New Users ", actual: @total.sign_ups, goal: @monthly_user_goal, complete: ((@total.sign_ups.to_f/@monthly_user_goal.to_f) * 100).round(2), color:"blue" } %>

    <%= render partial: "/common/goal", :locals => {name: "Active Users ", actual: @total.active_users, goal: @monthly_active_user_goal, complete: ((@total.active_users.to_f/@monthly_active_user_goal.to_f) * 100).round(2), color:"green" } %> 
      
</div><!-- /.col -->